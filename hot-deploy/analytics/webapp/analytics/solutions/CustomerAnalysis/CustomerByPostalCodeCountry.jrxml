    <parameter name="queryConditions" isForPrompting="false" class="java.lang.String">
        <defaultValueExpression ><![CDATA["1=1"]]></defaultValueExpression>
    </parameter>
    <queryString><![CDATA[SELECT
       SQ.Country as Country,
       SQ.PostalCode as PostalCode,
        COUNT(SQ.CustomerDimKey)AS NumberOfCustomers,
        SUM(SQ.LifetimeNumberOrders) AS LifetimeNumberOrders,
        SUM(SQ.LifetimeOrderTotal) AS LifetimeOrderTotal,
        SUM(SQ.LifetimeNumberReturns) AS LifetimeNumberReturns,
        SUM(SQ.LifetimeReturnTotal) AS LifetimeReturnTotal,
        SUM(SQ.LifetimeNetOrderValue) AS LifetimeNetOrderValue
    FROM (
        SELECT
            CNF.CUSTOMER_DIM_KEY AS CustomerDimKey,
            CD.CUSTOMER_NAME AS CustomerName,
            CD.COUNTRY AS Country,
            CD.STATE AS State,
            CD.CITY AS City,
            CD.POSTAL_CODE AS PostalCode,
            CD.POSTAL_CODE3DIGIT AS PostalCode3digit,
            LIFETIME_NUMBER_OF_ORDERS AS LifetimeNumberOrders,
            LIFETIME_ORDER_TOTAL AS LifetimeOrderTotal,
            LIFETIME_NUMBER_OF_RETURNS AS LifetimeNumberReturns,
            LIFETIME_RETURN_TOTAL AS LifetimeReturnTotal,
            LIFETIME_NET_ORDER_VALUE AS LifetimeNetOrderValue
        FROM CUSTOMER_HISTORY_FACT CNF, CUSTOMER_DIMENSION CD, (SELECT MAX(AS_OF_DATE_KEY) AS MaxDate FROM CUSTOMER_HISTORY_FACT) T
        WHERE CNF.CUSTOMER_DIM_KEY = CD.CUSTOMER_DIM_KEY AND CNF.AS_OF_DATE_KEY = MaxDate
        GROUP BY CNF.CUSTOMER_DIM_KEY
    ) SQ
    WHERE $P!{queryConditions}
    GROUP BY
        Country, PostalCode
    ORDER BY
        Country, PostalCode
    ]]></queryString>
