  <order>
  <hop> <from>Get customer sales (top 10)</from><to>Table output</to><enabled>Y</enabled> </hop>  </order>
  <step>
    <name>Get customer sales (top 10)</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>analytics</connection>
    <sql>
SELECT
  cd.customer_dim_key,
  chf.organization_id,
  cd.customer_name,
  sum( chf.lifetime_net_order_value ) AS SALES_VALUE
FROM CUSTOMER_HISTORY_FACT chf 
JOIN CUSTOMER_DIMENSION cd ON cd.customer_dim_key = chf.customer_dim_key 
WHERE 
  chf.as_of_date_key = (SELECT MAX(AS_OF_DATE_KEY) FROM CUSTOMER_HISTORY_FACT)
GROUP BY cd.customer_name 
ORDER BY SALES_VALUE DESC
LIMIT 10</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>Y</lazy_conversion_active>
